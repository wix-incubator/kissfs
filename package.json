{
  "name": "reactive-fs",
  "version": "0.0.1",
  "description": "javascript file-system read/write/watch API and basic implementations",
  "main": "dist/src/index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -d",
    "build:watch": "tsc -d -w",
    "bundle": "webpack --progress",
    "pretest": "npm run clean && npm run build && npm run bundle",
    "test": "npm run test:server && npm run test:browser",
    "test:server": "mocha --reporter mocha-env-reporter dist/test/index --recursive",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server --progress"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/reactive-fs.git"
  },
  "keywords": [
    "file",
    "folder",
    "abstraction",
    "watch"
  ],
  "author": "Amir Arad (amira@wix.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/reactive-fs/issues"
  },
  "homepage": "https://github.com/wix/reactive-fs#readme",
  "devDependencies": {
    "@types/bluebird": "3.0.35",
    "@types/chai": "3.4.34",
    "@types/chai-as-promised": "0.0.29",
    "@types/lodash": "4.14.39",
    "@types/mocha": "2.2.33",
    "@types/node": "6.0.48",
    "@types/sinon": "1.16.32",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-reporter": "1.0.13",
    "karma-firefox-launcher": "1.0.0",
    "karma-ie-launcher": "1.0.0",
    "karma-mocha": "1.3.0",
    "karma-safari-launcher": "1.0.0",
    "mkdirp": "0.5.1",
    "mocha": "3.1.2",
    "mocha-env-reporter": "2.0.4",
    "mocha-loader": "1.0.0",
    "ncp": "2.0.0",
    "rimraf": "2.5.4",
    "sinon": "1.17.6",
    "ts-loader": "1.2.1",
    "typescript": "2.0.9",
    "webpack": "2.1.0-beta.25",
    "webpack-dev-server": "2.1.0-beta.10"
  },
  "dependencies": {
    "bluebird": "3.4.6",
    "eventemitter3": "2.0.2",
    "lodash": "4.17.2"
  }
}
