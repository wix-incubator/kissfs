{
  "name": "kissfs",
  "version": "0.0.72",
  "description": "Extensible and reactive file-system library that keeps it simple",
  "main": "dist/src/nodejs.js",
  "browser": "dist/src/browser.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc -d",
    "build:watch": "tsc -d -w",
    "bundle": "webpack --progress",
    "pretest": "npm run clean && npm run build && npm run bundle",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "mocha --reporter mocha-env-reporter dist/test/nodejs",
    "test:browser": "karma start --single-run",
    "start": "webpack-dev-server --progress"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:wix/kissfs.git"
  },
  "keywords": [
    "file",
    "folder",
    "abstraction",
    "watch"
  ],
  "author": "Amir Arad (amira@wix.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wix/kissfs/issues"
  },
  "homepage": "https://github.com/wix/kissfs#readme",
  "devDependencies": {
    "@types/autobahn": "0.9.34",
    "@types/bluebird": "3.5.0",
    "@types/bluebird-retry": "0.0.32",
    "@types/chai": "3.4.35",
    "@types/chai-as-promised": "0.0.30",
    "@types/fs-extra": "2.0.0",
    "@types/micromatch": "2.3.29",
    "@types/lodash": "4.14.55",
    "@types/mocha": "2.2.40",
    "@types/node": "7.0.5",
    "@types/sinon": "1.16.35",
    "@types/tmp": "0.0.32",
    "bluebird-retry": "0.10.1",
    "chai": "3.5.0",
    "chai-as-promised": "6.0.0",
    "chai-subset": "1.5.0",
    "json-loader": "0.5.4",
    "karma": "1.5.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-env-reporter": "1.0.13",
    "karma-firefox-launcher": "1.0.1",
    "karma-mocha": "1.3.0",
    "mkdirp": "0.5.1",
    "mocha": "3.2.0",
    "mocha-env-reporter": "2.0.4",
    "mocha-loader": "1.1.1",
    "ncp": "2.0.0",
    "npminject": "1.0.238",
    "rimraf": "2.6.1",
    "sinon": "2.0.0-pre.6",
    "source-map-support": "0.4.13",
    "tmp": "0.0.31",
    "ts-loader": "2.0.2",
    "typescript": "2.1.6",
    "webpack": "1.14.0",
    "webpack-dev-server": "1.16.2"
  },
  "dependencies": {
    "autobahn": "0.11.2",
    "bluebird": "3.5.0",
    "chokidar": "1.6.1",
    "eventemitter3": "2.0.2",
    "fs-extra": "2.1.2",
    "is-glob": "3.1.0",
    "klaw": "1.3.1",
    "lodash": "4.17.4",
    "micromatch": "2.3.11",
    "wamp-server": "0.0.6"
  },
  "optionalDependencies": {
    "bufferutil": "1.2.1",
    "utf-8-validate": "1.2.1"
  }
}
